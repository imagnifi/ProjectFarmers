<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>Farmer</portlet-name>
		<display-name>Farmer</display-name>
		<portlet-class>ru.imagnifi.controllers.Farmer</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/jsp/Farmer/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>Farmer</title>
			<short-title>Farmer</short-title>
			<keywords>Farmer</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>District</portlet-name>
		<display-name>District</display-name>
		<portlet-class>ru.imagnifi.controllers.District</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/jsp/District/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>District</title>
			<short-title>District</short-title>
			<keywords>District</keywords>
		</portlet-info>
	</portlet>

</portlet-app>
